    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "express-winston": "^4.2.0",
    "firebase": "^9.19.1",
    "hubspot": "^2.3.14",
    "jsonwebtoken": "^9.0.0",
    "mkdirp": "^1.0.4",
