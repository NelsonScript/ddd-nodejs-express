{
  "name": "hubspot",
  "type": "commonjs",
  "scripts": {
    "debug": "export DEBUG=* && npm run start",
    "dev": "nodemon --watch src ./src/index.ts",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint --fix --ext .js,.ts .",
    "start": "NODE_PATH=./build node -r ts-node/register/transpile-only -r tsconfig-paths/register build/index.js",
    "swagger": "tsoa spec",
    "swagger:dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger:build": "npm run swagger",
    "test": "echo WTF?",
    "watch": "tsc --watch"
  },
